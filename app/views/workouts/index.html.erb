<div class="container">
  <h1>Training Log</h1>
  <p>Have you worked out today?</p></br>

  <div class="row">
    <% @workouts.each do |workout| %>
      <div class="col-sm-6">
      

  <h2><%= link_to workout.date, workout %></h2>

  <h3><%= workout.workout %></h3>

        </div>
      </div><br>
    <% end %>
    </div>
<%= link_to 'New Workout', new_workout_path %>
</div>